<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout8"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF0E8"
    tools:context=".FragmentMyinfo_2_addBo" >

    <Button
        android:id="@+id/btn_no"
        android:layout_width="102dp"
        android:layout_height="47dp"
        android:layout_marginStart="84dp"
        android:layout_marginBottom="32dp"
        android:text="취소"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_ok"
        android:layout_width="102dp"
        android:layout_height="49dp"
        android:text="등록"
        app:layout_constraintBottom_toBottomOf="@+id/btn_no"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.235"
        app:layout_constraintStart_toEndOf="@+id/btn_no"
        app:layout_constraintTop_toTopOf="@+id/btn_no"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="303dp"
        android:layout_height="73dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="가입된 유아보험 조회 / 등록"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txt_1"
        android:layout_width="162dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="136dp"
        android:gravity="center"
        android:text="보험사"
        android:textColor="#3A3939"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="160dp"
        android:layout_height="35dp"
        android:layout_marginEnd="20dp"
        android:background="#E3E0E0"
        android:dropDownWidth="200dp"
        android:spinnerMode="dropdown"
        app:layout_constraintBottom_toBottomOf="@+id/txt_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txt_1"
        app:layout_constraintVertical_bias="0.0"></Spinner>

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="160dp"
        android:layout_height="35dp"
        android:layout_marginEnd="20dp"
        android:background="#E3E0E0"
        app:layout_constraintBottom_toBottomOf="@+id/txt_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txt_2"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/txt_3"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="280dp"
        android:gravity="center"
        android:hint="가입자명"
        android:textColor="#3A3939"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txt_4"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="348dp"
        android:gravity="center"
        android:hint="생년월일 ex: 19971102"
        android:textColor="#3A3939"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_lookup"
        android:layout_width="132dp"
        android:layout_height="45dp"
        android:text="조회하기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.749" />

    <EditText
        android:id="@+id/txt_2"
        android:layout_width="159dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="208dp"
        android:foregroundGravity="center"
        android:gravity="center"
        android:text="보험이름"
        android:textColor="#3A3939"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_jo"
        android:layout_width="213dp"
        android:layout_height="36dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:textColor="#474545"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_lookup" />

</androidx.constraintlayout.widget.ConstraintLayout>