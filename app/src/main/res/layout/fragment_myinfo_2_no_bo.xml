<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout7"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF0E8"
    tools:context=".FragmentMyinfo_2_noBo" >

    <View
        android:id="@+id/textView63"
        android:layout_width="375dp"
        android:layout_height="2px"
        android:layout_marginTop="236dp"
        android:background="#DCDBDB"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/textView62"
        android:layout_width="375dp"
        android:layout_height="2px"
        android:layout_marginTop="372dp"
        android:background="#DCDBDB"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_backinfo"
        android:layout_width="65dp"
        android:layout_height="29dp"
        android:src="@drawable/reft"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.038" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="228dp"
        android:layout_height="148dp"
        android:src="@drawable/logomain"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txt_main12"
        android:layout_width="313dp"
        android:layout_height="62dp"
        android:layout_marginBottom="380dp"
        android:gravity="center"
        android:text="등록된 보험정보가 없습니다."
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txt_main4"
        android:layout_width="384dp"
        android:layout_height="32dp"
        android:layout_marginBottom="312dp"
        android:gravity="center"
        android:text="가입하신 보험 정보를 등록하고 편하게 확인해보세요! "
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.592"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txt_main5"
        android:layout_width="364dp"
        android:layout_height="59dp"
        android:layout_marginBottom="144dp"
        android:gravity="center|left"
        android:text="아직 어린이보험에 가입되어있지 않으시다면           AIM 연계보험을 추천드립니다."
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txt_main8"
        android:layout_width="331dp"
        android:layout_height="32dp"
        android:layout_marginBottom="80dp"
        android:gravity="center|left"
        android:text="● AIM 회원 연계 보험료 할인 이벤트"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.287"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_intro_kidBo"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:layout_marginTop="188dp"
        android:background="@drawable/solid_button"
        android:text="어린이보험 소개"
        android:textColor="#594B4B"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_addBo" />

    <TextView
        android:id="@+id/txt_main6"
        android:layout_width="331dp"
        android:layout_height="32dp"
        android:layout_marginBottom="112dp"
        android:gravity="center|left"
        android:text="● 연계보험 가입시 6개월 정액권 쿠폰 지급"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.287"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_addBo"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/solid_button"
        android:text="보험정보 등록"
        android:textColor="#594B4B"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_main12" />

</androidx.constraintlayout.widget.ConstraintLayout>