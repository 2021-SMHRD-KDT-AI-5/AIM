        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txt_main"
        android:layout_width="260dp"
        android:layout_height="64dp"
        android:layout_marginBottom="340dp"
